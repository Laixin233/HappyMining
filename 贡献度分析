import pandas as pd
import matplotlib.pyplot as plt
data=pd.read_excel(,index_col='')
data=data[''].copy()#对谁做帕累托图
data=data.sort_values(ascending=False)
plt.figure()
data.plot(kind='bar')
plt.ylabel('')#y轴名字
p=1.0*data.cumsum()/data.sum()
p.plot(color='r',secondary_y=True,style='-o',linewidth=2)
plt.ylabel('')#贡献度名字
plt.show()
